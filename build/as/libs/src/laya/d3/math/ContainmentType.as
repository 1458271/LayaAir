package laya.d3.math {

	/**
	 * <code>ContainmentType</code> 类用于定义空间物体位置关系。
	 */
	public class ContainmentType {

		/**
		 * 不相交
		 */
		public static var Disjoint:Number;

		/**
		 * 包含
		 */
		public static var Contains:Number;

		/**
		 * 相交
		 */
		public static var Intersects:Number;
	}

}
