
/**
 * Magnification filter.  Valid values correspond to WebGL enums: 9728 (NEAREST) and 9729 (LINEAR)
 */
package laya.gltf {

	public class glTFTextureMagFilter {

		/**
		 * Nearest
		 */
		public static var NEAREST:Number = 9728;

		/**
		 * Linear
		 */
		public static var LINEAR:Number = 9729;

	}
}