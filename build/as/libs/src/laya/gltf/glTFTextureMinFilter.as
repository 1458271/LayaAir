
/**
 * Minification filter.  All valid values correspond to WebGL enums
 */
package laya.gltf {

	public class glTFTextureMinFilter {

		/**
		 * Nearest
		 */
		public static var NEAREST:Number = 9728;

		/**
		 * Linear
		 */
		public static var LINEAR:Number = 9729;

		/**
		 * Nearest Mip-Map Nearest
		 */
		public static var NEAREST_MIPMAP_NEAREST:Number = 9984;

		/**
		 * Linear Mipmap Nearest
		 */
		public static var LINEAR_MIPMAP_NEAREST:Number = 9985;

		/**
		 * Nearest Mipmap Linear
		 */
		public static var NEAREST_MIPMAP_LINEAR:Number = 9986;

		/**
		 * Linear Mipmap Linear
		 */
		public static var LINEAR_MIPMAP_LINEAR:Number = 9987;

	}
}